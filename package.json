{
  "name": "@amzn/bilal-app",
  "version": "1.0.0",
  "dependencies": {
    "@amzn/awsui-components-react": "^3.0.878",
    "@amzn/harmony-react-tutorials": "^1.0.26",
    "ajv": "^6.12.5",
    "env-cmd": "^10.1.0",
    "gh-pages": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0"
  },
  "devDependencies": {
    "@amzn/harmony-types": "^1.0.36",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^28.1.6",
    "@types/node": "^16.11.48",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "react-scripts": "^5.0.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test-react": "react-scripts test --env=jsdom",
    "test": "echo no test",
    "eject": "react-scripts eject",
    "build-harmony-app": "build-harmony --templateName react",
    "publish": "build-harmony --templateName react --brazil-build",
    "build:staging": "env-cmd -f .env.staging npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "harmony": {
    "appName": "bilal-app",
    "jobCategories": [
      "developer"
    ],
    "headers": [
      {
        "name": "Content-Security-Policy",
        "value": "DEFAULT"
      }
    ]
  },
  "harmony-version": "1.7.101",
  "homepage": "https://github.com/bilalboultif/harmony-app"
}
